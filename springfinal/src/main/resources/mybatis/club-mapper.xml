<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    <mapper namespace="club">
    
    <insert id="add">
    
    insert into club(club_no,club_owner,club_category,zip_code_no,
    chat_room_no,club_name,club_explain,club_personnel,club_premium) 
    values(#{clubNo},#{clubOwner},#{clubCategory},#{zipCodeNo},
    #{chatRoomNo},#{clubName},#{clubExplain}#{clubPersonnel},#{clubPremium})
    
    </insert>
    
    <select id="sequence" resultType="int">
    	select club_seq.nextval from dual
    	</select>
    	

    
    
    </mapper>